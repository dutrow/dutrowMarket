<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
    version="3.1">

    <enterprise-beans>
        <session>
            <ejb-name>ParserEJB</ejb-name>
            <env-entry>
                <env-entry-name>vals/xmlFile</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>/xml/eSales-all.xml</env-entry-value>
            </env-entry>
        </session>
        <session>
            <ejb-name>SellerMgmtEJB</ejb-name>
            <env-entry>
                <env-entry-name>vals/checkItemInterval</env-entry-name>
                <env-entry-type>java.lang.Long</env-entry-type>
                <env-entry-value>10000</env-entry-value>
                <injection-target>
                    <injection-target-class>
                        dutrow.sales.ejb.SellerMgmtEJB
                    </injection-target-class>
                    <injection-target-name>
                        checkItemInterval
                    </injection-target-name>
                </injection-target>
            </env-entry>
            </session>
            
    </enterprise-beans>


</ejb-jar>
